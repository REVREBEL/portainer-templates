      "volumes": [
        { "bind": "/opt/docker/app_data/redis-commander", "container": "/redis-commander" }]
    },
    {
      "type": 1,
      "title": "redis",
      "description": "Open-source in-memory data structure store",
      "name": "redis",
      "platform": "linux",
      "logo": "https://res.cloudinary.com/revrebel/image/upload/v1654662118/RevRebel/App%20Icons/redis_qfjfpj.svg",
      "categories": [ "database" ],
      "env": [
        { "default": "6379", "label": "REDIS_PORT_NUMBER", "name": "REDIS_PORT_NUMBER" },
        { "default": "6380", "label": "REDIS_TLS_PORT", "name": "REDIS_TLS_PORT" },
        { "default": "yes", "label": "REDIS_TLS_ENABLED", "name": "REDIS_TLS_ENABLED" },
        { "default": "/opt/revrebel/redis/certs/redis.crt", "label": "REDIS_TLS_CERT_FILE", "name": "REDIS_TLS_CERT_FILE" },
        { "default": "/opt/revrebel/redis/certs/redis.key", "label": "REDIS_TLS_KEY_FILE", "name": "REDIS_TLS_KEY_FILE" },
        { "default": "/opt/revrebel/redis/certs/ca.crt", "label": "REDIS_TLS_CA_FILE", "name": "REDIS_TLS_CA_FILE" },
        { "default": "/opt/revrebel/redis/certs/redis.dh", "label": "REDIS_TLS_DH_PARAMS_FILE", "name": "REDIS_TLS_DH_PARAMS_FILE" },
        { "default": "no", "label": "REDIS_TLS_AUTH_CLIENTS", "name": "REDIS_TLS_AUTH_CLIENTS" },
        { "default": "/opt/revrebel/redis/certs/ca.crt", "label": "REDIS_TLS_CA_FILE", "name": "REDIS_TLS_CA_FILE" },
        { "default": "/opt/revrebel/redis/etc/users.acl", "label": "REDIS_ACLFILE", "name": "REDIS_ACLFILE" },
        { "default": "996", "label": "PUID", "name": "PUID" },
        { "default": "996", "label": "PGID", "name": "PGID" },
        { "default": "America/Los_Angeles", "label": "TZ", "name": "TZ" }
      ],
      "image": "redis:latest",
      "hostname": "redis",
      "command": "/opt/revrebel/scripts/redis/run.sh --maxmemory 100mb",
      "ports": [ "6380:6380/tcp", "6379:6379/tcp" ],
      "network": "docweb",
      "restart_policy": "unless-stopped",
      "volumes": [
        { "bind": "/opt/docker/app_data/redis/redis_data", "container": "/etc/redis/data/" },
        { "bind": "/opt/docker/app_data/redis/revrebel", "container": "/opt/revrebel" }]
    },
    {
      "type": 1,
      "title": "nginx-proxy-manager",
      "description": "Nginx Proxy Manager enables you to easily forward to your websites running at home or otherwise, including free SSL, without having to know too much about Nginx or Letsencrypt.",
      "name": "nginx-proxy-manager",
      "platform": "linux",
      "logo": "https://res.cloudinary.com/revrebel/image/upload/v1656859103/RevRebel/App%20Icons/nginx-proxy_guswuc.png",
      "categories": [ "docker" ],
      "hosts": [ "host.docker.internal:host-gateway" ],
      "env": [
        { "default": "/run/host-services/ssh-auth.sock", "label": "SSH_AUTH_SOCK", "name": "SSH_AUTH_SOCK" },
        { "default": "sameorigin", "label": "X_FRAME_OPTIONS", "name": "X_FRAME_OPTIONS" },
        { "default": "mariadb", "label": "DB_MYSQL_HOST", "name": "DB_MYSQL_HOST" },
        { "default": "3306", "label": "DB_MYSQL_PORT", "name": "DB_MYSQL_PORT" },
        { "default": "nginxproxy", "label": "DB_MYSQL_USER", "name": "DB_MYSQL_USER" },
        { "default": "password", "label": "DB_MYSQL_PASSWORD", "name": "DB_MYSQL_PASSWORD" },
        { "default": "nginxproxydb", "label": "DB_MYSQL_NAME", "name": "DB_MYSQL_NAME" },
        { "default": "1000", "label": "PUID", "name": "PUID" },
        { "default": "1000", "label": "PGID", "name": "PGID" },
        { "default": "America/Los_Angeles", "label": "TZ", "name": "TZ" },
        { "name": "DISABLE_IPV6", "label": "DISABLE_UPV6",
          "description": "option to disable ipv6",
          "select": [
            { "text": "NO", "value": "false", "default": "false" },
            { "text": "YES", "value": "true" } ]}
      ],
      "image": "jlesage/nginx-proxy-manager",
      "hostname": "nginx-proxy-manager",
      "ports": [ "80:8080/tcp", "81:8181/tcp", "443:4443/tcp" ],
      "network": "docweb",
      "restart_policy": "unless-stopped",
      "volumes": [
        { "container": "/data", "bind": "/opt/docker/app_data/nginx-proxy/data" },
        { "container": "/etc/nginx/conf.d", "bind": "/opt/docker/app_data/nginx-proxy/tmp/nginx" },
        { "container": "/templates", "bind": "/opt/docker/app_data/nginx-proxy/temp/nginx" },
        { "container": "/etc/letsencrypt", "bind": "/opt/docker/app_data/nginx-proxy/letsencrypt" }
      ],
      "healthcheck": { "test": [ "CMD", "/bin/check-health" ], 
	    "interval": "10s", 
 	    "timeout": "3s" }
      },
    { 
    "type": 1,
      "title": "sematext docker agent",
      "description": "Collect logs, metrics and docker events",
      "name": "sematext-docker-agent",
      "platform": "linux",
      "logo": "https://res.cloudinary.com/revrebel/image/upload/v1658779238/RevRebel/App%20Icons/sematext_ywn2mc.svg",
      "categories": [ "log management", "monitoring" ], "cap_add": "- SYS ADMIN",
      "env": [
        { "name": "LOGSENE_TOKEN", "label": "LOGSENE_TOKEN" },
        { "name": "SPM_TOKEN", "label": "SPM_TOKEN" },
        { "name": "INFRA_TOKEN", "label": "INFRA_TOKEN" },
        { "default": "https://spm-receiver.sematext.com", "name": "SERVER_BASE_URL", "label": "SERVER_BASE_URL" },
        { "default": "https://logsene-receiver.sematext.com", "name": "LOGS_RECEIVER_URL", "label": "LOGS_RECEIVER_URL" },
        { "default": "https://event-receiver.sematext.com", "name": "EVENT_RECEIVER_URL", "label": "EVENT_RECEIVER_URL" },
        { "default": "https://command.sematext.com", "name": "COMMAND_SERVER_URL", "label": "COMMAND_SERVER_URL" },
        { "default": "1000", "label": "PUID", "name": "PUID" },
        { "default": "1000", "label": "PGID", "name": "PGID" },
        { "default": "America/Los_Angeles", "label": "TZ", "name": "TZ" }
      ],
      "image": "sematext/agent:latest",
      "hostname": "sematext-docker-agent",
      "privileged": true,
      "network": "docweb",
      "restart_policy": "unless-stopped",
      "volumes": [
        { "container": "/host/sys", "bind": "/", "readonly": "true" },
        { "container": "/etc/passwd", "bind": "/etc/passwd", "readonly": "true" },
        { "container": "/etc/group", "bind": "/etc/group", "readonly": "true" },
        { "container": "/var/run", "bind": "/var/run" },
        { "container": "/sys/kernel/debug", "bind": "/sys/kernel/debug" },
        { "container": "/sys", "bind": "/host/sys","readonly": "true" },
        { "container": "/dev", "bind": "/hostfs/dev", "readonly": "true" },
        { "container": "/var/run/docker", "bind": "/var/run/docker.sock", "readonly": "true" }]
    },
